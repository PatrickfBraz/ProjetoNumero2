{
  "storage": {
    "dataExtension": "CSV",
    "encoding": "utf-8",
    "remote": false,
    "localPath": "/tmp/",
    "storageEngine": "csvGenerator"
  },
  "logger": {
    "storage": {
      "active": false,
      "local": {
        "path": "/tmp/"
      }
    },
    "format": "{\"timestamp\": \"%(asctime)s\", \"level\": \"%(levelname)s\",\"location\": \"%(module)s:%(lineno)d\", \"message\": \"%(message)s\"}",
    "level": "DEBUG"
  },
  "scraper": {
    "requestTimeout": 30,
    "maxRetries": 3,
    "scraperEngine": "httpScraper"
  }
}